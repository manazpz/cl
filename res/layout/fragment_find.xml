<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ececec"
    android:orientation="vertical"
    tools:context="com.example.Fragment.FindFragment" >
    
    <RelativeLayout
        android:id="@+id/relativeLayout1"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:background="@drawable/my_login_bg" >

        <TextView
            android:id="@+id/tv_tital"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="发现"
            android:ellipsize="end"
            android:maxLines="2"
            android:maxWidth="200dp"
            android:textSize="20sp" />
    </RelativeLayout>
    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/top_bar"
        android:layout_marginBottom="5.0dip"
        android:background="@color/app_gray"
        android:scrollbars="none" >
        
      <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/common_divider_height"
                android:layout_marginTop="@dimen/common_divider_height"
                android:background="@color/app_gray"
                android:orientation="vertical" >

    <RelativeLayout 
        android:id="@+id/rl_Gools"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp">

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:padding="3dp"
            android:src="@android:drawable/ic_menu_view" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imageView1"
            android:text="商品圈" 
            android:textColor="@color/textColor_32"
            android:textSize="@dimen/section_text_size"/>
    </RelativeLayout>
    
    <View 
        android:layout_width="match_parent"
        android:layout_height="10dp"
        />
	<RelativeLayout 
        android:id="@+id/rl_Activity"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp">

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:src="@android:drawable/ic_menu_crop"
            android:padding="3dp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imageView2"
            android:text="活动" 
            android:textColor="@color/textColor_32"
            android:textSize="@dimen/section_text_size"/>
    </RelativeLayout>
    
    
    
	<View 
        android:layout_width="match_parent"
        android:layout_height="10dp"
        />
	<RelativeLayout 
        android:id="@+id/rl_Ditu"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:src="@android:drawable/ic_menu_mapmode"
            android:padding="3dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imageView5"
            android:text="我的位置" 
            android:textColor="@color/textColor_32"
            android:textSize="@dimen/section_text_size"/>
    </RelativeLayout>
     </LinearLayout>
     
     <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/common_divider_height"
                android:layout_marginTop="@dimen/common_divider_height"
                android:background="@color/app_gray"
                android:orientation="vertical" >        
    
	<RelativeLayout 
        android:id="@+id/rl_zxing"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:src="@android:drawable/ic_menu_compass"
            android:padding="3dp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imageView3"
            android:text="扫一扫" 
            android:textColor="@color/textColor_32"
            android:textSize="@dimen/section_text_size"/>
    </RelativeLayout>
    
	<View 
        android:layout_width="match_parent"
        android:layout_height="10dp"
        />
	<RelativeLayout 
        android:id="@+id/rl_Kefu"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:src="@android:drawable/ic_menu_call"
            android:padding="3dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imageView4"
            android:text="客服" 
            android:textColor="@color/textColor_32"
            android:textSize="@dimen/section_text_size"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="2.0dip"
            android:layout_toRightOf="@id/textView4"
            android:text="(8:00-22:00)"
            android:textColor="@color/textColor_a0"
            android:textSize="14.0sp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:text="1213-132" />

    </RelativeLayout>
    
    <View 
        android:layout_width="match_parent"
        android:layout_height="10dp"
        />
	<RelativeLayout 
        android:id="@+id/rl_Clean"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp">

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:src="@android:drawable/ic_menu_save"
            android:padding="3dp" />
        <TextView
            android:id="@+id/cache_size"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="5.0dip"
            android:text=""
            android:textColor="@color/textColor_a0"
            android:textSize="17.0sp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imageView6"
            android:text="清除缓存" 
            android:textColor="@color/textColor_32"
            android:textSize="@dimen/section_text_size"/>
    </RelativeLayout>
    </LinearLayout>

     <LinearLayout
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:orientation="vertical" >
	
      <RelativeLayout 
        android:id="@+id/rl_Files"
        android:background="#fff"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="5dp">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:src="@android:drawable/ic_menu_save"
            android:padding="3dp" />
        

        <TextView
            android:id="@+id/tv_myfile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/imageView7"
            android:text="我的文件" 
            android:textColor="@color/textColor_32"
            android:textSize="@dimen/section_text_size"/>
    </RelativeLayout>
     </LinearLayout>

  </LinearLayout>
 </ScrollView>
	
</LinearLayout>
